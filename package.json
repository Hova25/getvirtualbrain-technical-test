{
  "name": "getvirtualbrain-technical-test",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter \"@getvirtualbrain-technical-test/*\" dev",
    "build": "pnpm  --filter \"@getvirtualbrain-technical-test/*\" build",
    "lint": "pnpm --parallel  --filter \"@getvirtualbrain-technical-test/*\" lint",
    "test": "pnpm --parallel  --filter \"@getvirtualbrain-technical-test/*\" test",
    "clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "format": "prettier --cache --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript-eslint": "^8.39.0"
  }
}
